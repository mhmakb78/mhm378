

#include <iostream>
using namespace std;

long long factorial(int n){
	if(n == 0 || n == 1)
		return 1;
	else
		return n * factorial(n-1);
}


int main(int argc, char** argv) {

	int n;

    cout << "adad mored nazar ra vared konid: ";
    cin >> n;

    cout << n << "! = " << factorial(n);
    return 0;
}
